/* pages/cssCase/circleHead/index.wxss */
.head {
  width: 100%;
  height: 231rpx;
  overflow: hidden;
  position: relative;
  z-index: -1;
}

.head::after {
  content: '';
  width: 140%;
  height: 231rpx;
  position: absolute;
  left: -20%;
  top: 0;
  border-radius: 0 0 50% 50%;
  background: #FA871E;
}