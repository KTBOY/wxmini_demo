/* pages/subPack/loading/index3.wxss */
page{
  background-image: linear-gradient(to right, #a8edea 0%, #fed6e3 100%);
  padding-bottom: 100rpx;
}
.tips{
  font-size: 26rpx;
  color: rgb(201, 138, 138);
  padding: 10rpx 40rpx;
}
.box{
  margin: 20rpx auto;
  width: 685rpx;
  height: 200rpx;
  background: #ccd8d7;
  border-radius: 10rpx;
  box-shadow: 0px 0px 30rpx 1rpx #fed6e3 inset;
  display: flex;
  align-items: center;
  justify-content: center;
  position: relative;
}
/* 1 */
.loader1{
  width: 48rpx;
  height: 96rpx;
  display: inline-block;
  position: relative;
  border: 3rpx solid #fff;
  animation: loading1 2s linear infinite alternate;
}
.loader1::before {
  content: '';
  position: absolute;
  top: -12rpx;
  left:50%;
  margin-left: -18rpx;
  width: 36rpx;
  height: 12rpx;
  background-color: #fff;
}

@keyframes loading1 {
  0% {
    box-shadow: 0 0 inset #fff;
  }
  60%{
    box-shadow: 0 -60px inset #d69999;
  }
  100% {
    box-shadow: 0 -96px inset #e47777;
  }
}
/* 2 */
.loader2{
  width: 64rpx;
  height: 64rpx;
  display: inline-block;
  position: relative;
  background: #fff;
  animation: loading2 2s ease infinite alternate-reverse;
  /* alternate-reverse 动画在奇数次（1、3、5...）反向播放，在偶数次（2、4、6...）正向播放。 */
}
@keyframes loading2 {
  0% {
    transform: scale(1) rotate(0deg)
  }
  100% {
    transform: scale(.1) rotate(360deg)
  }
}
/* 3 */
.loader3 {
  width: 24rpx;
  height: 24rpx;
  box-shadow: 0 30rpx, 0 -30rpx;
  border-radius: 4rpx;
  background: currentColor;
  display: inline-block;
  position: relative;
  color: #fff;
  left: -30rpx;
  animation: loading3 2s ease infinite;
}

.loader3::before, .loader3::after {
  content: '';
  width: 24rpx;
  height: 24rpx;
  box-shadow: 0 30rpx, 0 -30rpx;
  border-radius: 4rpx;
  background: currentColor;
  color: #fff;
  position: absolute;
  left: 30rpx;
  top: 0;
  animation: loading3 2s 0.2s ease infinite;
}
.loader3::after {
  left: 60rpx;
  animation-delay: 0.4s;
}

@keyframes loading3 {
  0% {
    top: 0;
    color: rgba(255, 255, 255, 1)
  }
  50% {
    top: 30rpx;
    color: rgba(255, 255, 255, 0.2)
  }
  100% {
    top: 0;
    color: rgba(255, 255, 255, 1)
  }
}
/* 4 */
.loader4{
  width: 80rpx;
  display: flex;
  flex-wrap: wrap;
  animation: rotate 3s linear infinite;
}

@keyframes rotate{
  to{
      transform: rotate(360deg);
  }
}

.loader4 .l-item{
  width: 32rpx;
  height: 32rpx;
  background-color: #d3ebea;
  margin: 4rpx;
  animation: scale 1.5s linear infinite;
}
.loader4 .l-item:nth-child(2){
  background-color: #99dddb;
}
.loader4 .l-item:nth-child(3){
  background-color: #99dddb;
}
.loader4 .l-item:nth-child(4){
  background-color: #d3ebea;
}

@keyframes scale{
  50%{
      transform: scale(1.4);
  }
}
/* 5 */
.loader5{
  width: 80rpx;
  display: flex;
  flex-wrap: wrap;
  animation: rotate 3s linear infinite;
}


.loader5 .l-item{
  width: 32rpx;
  height: 32rpx;
  background-color: #aed4a4;
  margin: 4rpx;
  border-radius: 50% 50% 0 50%;
  transform-origin: bottom right;
}
.loader5 .l-item:nth-child(2){
  background-color: #84d488;
  border-radius: 50% 50% 50% 0;
  transform-origin: bottom left;
}
.loader5 .l-item:nth-child(3){
  background-color: #416937;
  border-radius: 50% 0 50% 50%;
  transform-origin: top right;
}
.loader5 .l-item:nth-child(4){
  background-color: #82af77;
  border-radius: 0 50% 50% 50%;
  transform-origin: top left;
}

/* 6 */
.loader6 .dot{
  background-color:#39b3d8;
  display: inline-block;
  width: 30rpx;
  height: 30rpx;
  border-radius: 8rpx;
  margin: 0 5rpx;
  opacity: 0;
 }
 /* 7 */
 .loader7{
    transform: scale(0.7);
    width: 600rpx;
    position: relative;
 }
 .loader7 view {
  position: absolute;
  width : 80rpx;
  height: 80rpx;
  background-color: var(--color);
  left: calc((var(--i) - 1) * 173rpx);
  /* 父元素宽600rpx 4*80+3x=600 x=93  即 80+73 */
  border-radius: 20rpx;
  animation : animate 1s linear infinite;
  display : flex;
  justify-content: center;
  align-items: center;
}

@keyframes animate {
  0% {
      transform: rotate(-45deg);
  }
  100% {
      transform: rotate(45deg);
  }
}

.loader7 view::after {
  position : absolute;
  content  : '';
  width : 80rpx;
  height : 80rpx;
  background-color: var(--color);
  top: -120rpx;
  border-radius : 20rpx;
  animation: animate_show 4s steps(1, end) infinite;
  animation-delay : calc((5 - var(--i)) * -1s);
}

@keyframes animate_show {

  0% {
      opacity: 1;
  }

  25%,50%,75%,100% {
      opacity: 0;
  }
}
.loader7 view::before {
  position : absolute;
  content : '';
  width: 40rpx;
  height : 40rpx;
  border-radius : 50%;
  background-color: #fff;
}
/* 8 */
.loader8{
  width: 130rpx;
  height: 130rpx;
  border: 10rpx solid #65a3c0;
  box-shadow: 0 0 0 5rpx #4be089;
  border-radius: 50%;
}
.wave {
  position: relative;
  width: 100%;
  height: 100%;
  background-color: #ddad9b;
  border-radius: 50%;
  box-shadow: inset 0 0 50rpx #4be089;
  overflow: hidden;
  z-index: 0;
}

.wave::before,
.wave::after {
  position: absolute;
  content: "";
  width: 200%;
  height: 200%;
  top: 0;
  left: 50%;
  transform: translate(-50%, -50%);
}

.wave::before {
  border-radius: 30%;
  background-color: #3a9168;
  animation: animate0 5s linear infinite;
}

.wave::after {
  border-radius: 40%;
  background-color: #1fb690;
  animation: animate0 5s linear infinite;
}

@keyframes animate0 {
  0% {
    top: 0;
    transform: translate(-50%, -50%) rotate(0deg);
  }
  100% {
    top: -70rpx;
    transform: translate(-50%, -50%) rotate(360deg);
  }
}
/* 9 */
.loader9{
  transform: scale(0.7);
  width: 600rpx;
  position: relative;
}
.loader9 view {
  position: absolute;
  width : 80rpx;
  height: 80rpx;
  background-color: var(--color);
  left: calc((var(--i) - 1) * 173rpx);
  border-radius: 20rpx;
  animation : animate 4s linear infinite;
  display : flex;
  justify-content: center;
  align-items: center;
  animation-delay : calc((5 - var(--i)) * -1s);
}

@keyframes animate {
  0% {
    transform: rotate(-45deg) scale(0.8);
  }
  100% {
    transform: rotate(45deg) scale(1);
  }
}


.loader9 view::before {
  position : absolute;
  font-size: 40rpx;
}

.loader9 view:nth-child(1):before{
  content : '+';
  color: #b88320;
}
.loader9 view:nth-child(2):before{
  content : '-';
  color: #810d5e;
}
.loader9 view:nth-child(3):before{
  content : 'x';
  color: #7496d4;
}
.loader9 view:nth-child(4):before{
  content : '/';
  color: #18ca68;
}
/* 10 */
.loader10{
  position: absolute; 
  top: 50%;
  left: 50%;
  height: 40rpx; 
  width: 40rpx;
  transform: translate(-50%, -50%) rotate(45deg) translate3d(0,0,0);
  animation: animate10 1.2s ease-in-out infinite; /*动画: 名称 时间 播放线性 重复*/
}
@keyframes animate10{ 
  0%, 10%, 100%{
    height: 40rpx;
    width: 40rpx;
  }
 
  65%{
    height: 70rpx;
    width: 70rpx;
  }
}
.a{
  position: absolute;
  display: block; 
  width: 20rpx;
  height: 20rpx;
  animation: rotate10 1.2s linear both infinite;
}
@keyframes rotate10{
  0%, 30% {
    transform: rotate(0);
  } 
  65%{
    transform: rotate(-40deg);
  }
}
.a:nth-child(1){ 
  background: #1ac9ff;
  left: 0;
  top: 0;
}
.a:nth-child(2){ 
  background: #f78fb3;
  right: 0;
  top: 0;
}
.a:nth-child(3){
  background: #af49b6;
  left: 0;
  bottom: 0;
}
.a:nth-child(4){
  background: #e03870;
  right: 0;
  bottom: 0;
}

/* 11 */
.loader11{
  position: absolute;
  top: 50%;
  left: 50%;
  -webkit-transform: translate(-50%, -50%);
  transform: translate(-50%, -50%);
  display: -webkit-box;
  display: -ms-flexbox;
  display: flex;
  -webkit-box-pack: center;
  -ms-flex-pack: center;
  justify-content: center;
  width: 100%;
  max-width: 500rpx;
  height: 200rpx;
}
.loader11 .box {
  width: 30rpx;
  height: 30rpx;
  margin: 0 10rpx;
  border-radius: 0%;
  -webkit-transform: translateY(0) rotate(0deg);
  transform: translateY(0) rotate(0deg);
  -webkit-animation: change 1s infinite alternate;
  animation: change 1s infinite alternate;
}

.loader11 .box:nth-child(1) {
  background-color: #574b90;
  -webkit-animation-delay: 0.1s;
  animation-delay: 0.1s;
}

.loader11 .box:nth-child(2) {
  background-color:#f78fb3;
  -webkit-animation-delay: 0.2s;
  animation-delay: 0.2s;
}

.loader11 .box:nth-child(3) {
  background-color: #3dc1d3;
  -webkit-animation-delay: 0.3s;
   animation-delay: 0.3s;
}

.loader11 .box:nth-child(4) {
  background-color: #e66767;
  -webkit-animation-delay: 0.4s;
  animation-delay: 0.4s;
}

.loader11 .box:nth-child(5) {
  background-color: #303952;
  -webkit-animation-delay: 0.5s;
  animation-delay: 0.5s;
}

.loader11 .box:nth-child(6) {
  background-color: #f3a683;
  -webkit-animation-delay: 0.6s;
  animation-delay: 0.6s;
}

@-webkit-keyframes change {
  0% {
    -webkit-transform: translateY(0px) rotate(0deg);
    transform: translateY(0px) rotate(0deg);
    border-radius: 0%;
  }
  50% {
    -webkit-transform: translateY(90px) rotate(180deg);
    transform: translateY(90px) rotate(180deg);
    border-radius: 50%;
  }
  100% {
    -webkit-transform: translateY(0px) rotate(360deg);
    transform: translateY(0px) rotate(360deg);
    border-radius: 0%;
  }
}

@keyframes change {
  0% {
    -webkit-transform: translateY(0px) rotate(0deg);
    transform: translateY(0px) rotate(0deg);
    border-radius: 0%;
  }
  50% {
    -webkit-transform: translateY(90px) rotate(180deg);
    transform: translateY(90px) rotate(180deg);
    border-radius: 50%;
  }
  100% {
    -webkit-transform: translateY(0px) rotate(360deg);
    transform: translateY(0px) rotate(360deg);
    border-radius: 0%;
  }
}